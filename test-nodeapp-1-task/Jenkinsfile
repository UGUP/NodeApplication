pipeline {
    agent any
    stages {
        stage('Clone') {
            steps {
                sshagent(['sshcredentials3']) {
                    git branch: 'main',
                    credentialsId: 'sshcredentials3',
                    url: 'git@github.com:UGUP/NodeApplication.git'
                }
            }
        }
    }
}
